<div class="panel-body" style="padding-top: 0">
	<table class="table table-hover table-bordered">
		<thead class="bg-gray">
			<tr>
				<th>Name</th>	
				<th>Status</th>	
				<!-- <th>Action</th>	 -->
			</tr>
		</thead>
		<tbody>
			<% roles.each do |role| %>
				<tr>
					<td><%= role.name %></td>
					<td><%= role.status %></td>
					<!-- <td><%#= role %></td> -->
				</tr>
			<% end %>
		</tbody>
	</table>
</div>